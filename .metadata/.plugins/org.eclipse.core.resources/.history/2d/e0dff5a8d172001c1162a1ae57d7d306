package com.xworkz.tour;

public class Test {
	
	public void run() {
		for (int h = 1; h <=3; h++) {
			
		}
		try {
			Thread.sleep(10);
		} catch (Exception e) {
			System.out.println("hello");
		}
		System.out.println(h+" ");
	}

	
	public static void main(String[] args) {
		hack_thread h1= new hack_thread();
		hack_thread h2= new hack_thread();
		h1.start();
		
		try {
			h1.join();
		} catch (Exception e) {
			System.out.println("hi");
		}
		h2.start();
	}
}
